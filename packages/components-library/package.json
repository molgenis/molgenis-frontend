{
  "name": "@molgenis-ui/components-library",
  "version": "2.0.3",
  "license": "LGPL-3.0",
  "homepage": "https://molgenis.org",
  "description": "Vue component library for molgenis web frontend",
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "styleguide"
